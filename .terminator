[global_config]
  title_hide_sizetext = True
  title_transmit_bg_color = "#204a87"
  title_receive_bg_color = "#5c3566"
  title_inactive_fg_color = "#555753"
  inactive_color_offset = 0.9259259259259259
[keybindings]
[profiles]
  [[default]]
    background_darkness = 0.7
    background_type = transparent
    cursor_color = "#aaaaaa"
    show_titlebar = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
[plugins]
